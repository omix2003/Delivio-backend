{
  "name": "delivery-network-backend",
  "version": "1.0.0",
  "description": "Backend API server for Centralized Delivery Network",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "node node_modules/prisma/build/index.js generate && tsc",
    "start": "node dist/server.js",
    "prestart": "chmod +x node_modules/.bin/prisma 2>/dev/null || true && echo 'Running migrations before server start...' && npx prisma migrate deploy || echo 'Migration failed, continuing anyway...'",
    "postinstall": "node node_modules/prisma/build/index.js generate || npx prisma generate || echo 'Prisma generate failed, will retry during build'",
    "build:vercel": "npx prisma generate && tsc",
    "build:render": "npm run build && chmod +x node_modules/.bin/prisma",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test:assignment": "ts-node src/scripts/test-assignment.ts",
    "test:location-updates": "ts-node src/scripts/test-location-updates.ts",
    "test:billing": "ts-node src/scripts/test-billing.ts",
    "test:billing:comprehensive": "ts-node src/scripts/test-billing-comprehensive.ts",
    "sync:wallet-revenue": "ts-node src/scripts/sync-wallet-revenue.ts",
    "fix:logistics-orders": "ts-node src/scripts/fix-logistics-orders.ts",
    "fix:drop-warehouse-id": "ts-node src/scripts/fix-drop-warehouse-id.ts",
    "send:payout-reminders": "ts-node src/scripts/send-payout-reminders.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bwip-js": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pdfkit": "^0.17.2",
    "prisma": "^6.1.0",
    "razorpay": "^2.9.6",
    "socket.io": "^4.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@vercel/node": "^3.2.29",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
